{
    "name": "@biconomy/fee-management",
    "version": "1.0.0",
    "description": "Fee management module to manage and monitor Native token in Master account accross chains",
    "main": "dist/fee-manager.js",
    "types": "dist/fee-manager.d.ts",
    "scripts": {
      "prebuild": "tslint -c tslint.json -p tsconfig.json --fix",
      "test:unit": "jest --coverage test/unit",
      "test:e2e": "jest --forceExit --silent test/e2e/",
      "test": "yarn test:unit && yarn test:e2e",
      "build": "tsc"
    },
    "repository": {
      "type": "git",
      "url": "git+https://github.com/bcnmy/fee-management.git"
    },
    "keywords": [
      "fee-management",
      "sdk",
      "cross-chain",
      "transfers",
      "swaps"
    ],
    "author": "Divya Nailwal",
    "license": "MIT",
    "bugs": {
      "url": "https://github.com/bcnmy/fee-management/issues"
    },
    "homepage": "https://github.com/bcnmy/fee-management#readme",
    "files": [
      "*.js",
      "/dist/**/*.js",
      "/dist/*.js",
      "/dist/**/*.d.ts",
      "/dist/abi/*.json"
    ],
    "dependencies": {
      "cross-fetch": "^3.1.5",
      "ethers": "^5.0.24",
      "promise": "^8.0.3",
      "tslint": "^6.1.3",
      "typescript": "^4.1.3",
      "mongoose": "^5.5.8",
      "mongo-mock": "^3.8.1"
    },
    "devDependencies": {
      "@types/mongoose": "^5.7.36",
      "@types/jest": "^27.4.1",
      "@types/node": "^14.14.25",
      "chai": "^4.3.6",
      "dotenv": "^16.0.0",
      "jest": "^27.5.1",
      "ts-jest": "^27.1.4",
      "ts-node": "^10.7.0"
    }
  }
  